CREATE TABLE `sequences` (   `value` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'The value of the sequence.',   PRIMARY KEY (`value`) ) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8 COMMENT='Stores IDs.' AUTO_INCREMENT=7;
