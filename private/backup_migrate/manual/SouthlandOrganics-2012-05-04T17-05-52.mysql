INSERT INTO `block_custom` VALUES ('13','For commercial poultry please visit us at <a href="http://www.litterlife.com/">LitterLife.com</a>','Litter Life More Information','pure_html'),('14','For larger quantities or distribution possibilities please call for a quote.\r\n<div class="phone">(800) 608-3755</div>','Volume / Distribution','pure_html'),('15','<img src="/soil_conditioner/secure_organics.png" width="128" height="128" alt="Secure Online Store"><div class="secure">Always</div><div class="secure">Secure</div>\r\n<div class="ssl">256 Bit SSL Encryption</div>\r\n<div class="cards">\r\n<img src="/soil_conditioner/visa-organic.png" width="64" height="64" alt="Visa Card">\r\n<img src="/soil_conditioner/mastercard-icon.png" width="64" height="64" alt="Master Card">\r\n<img src="/soil_conditioner/american-express-icon.png" width="64" height="64" alt="American Express">\r\n</div>','SSL and Cards (Checkout)','pure_html'),('16','<div class="so-cartlinks"><a class="update-cart" href="/cart">Update Shopping Cart</a></div>\r\n<div class="so-cartlinks"><a class="continue" href="/store">Continue Shopping</a></div>','Cart Links','pure_html');
