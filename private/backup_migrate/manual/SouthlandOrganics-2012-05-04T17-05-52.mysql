INSERT INTO `block_custom` VALUES ('1','Many of our products are <strong class="weak">USDA Certified Organic</strong> and carry the EPA Design for the Environment Seal.  We undergo rigorous testing so that our customers can be sure that they are purchasing top <strong class="weak">organic</strong> products for their <strong class="weak">garden</strong> and <strong class="weak">lawn.</strong>\r\n<a href="/soil-foodweb"><img alt="Organic SoilfoodWeb Certification" src="/soil_conditioner/soil-foodweb.gif" height="70" width="70"/></a> <a href="/epa-certification"><img alt="Southland Organics EPA Certification" src="/soil_conditioner/epa-certification.png" height="70" width="75"/></a><img alt="Southland Organics USDA Organic Certification" src="/soil_conditioner/USDA-Organic.png" height="70" width="70"/>','Organic Certification','full_html'),('2','<div class="webelieve">We Believe in a Sustainable Future.</div>\r\nOur products provide natural solutions to natural problems.<br />\r\nWe believe that nourishing the soil through proper environmental stewardship is the key to a healthier food supply.\r\n','Mission Statement','pure_html'),('3','<a href="/user/login">Login</a>','Modal Login','pure_html'),('4','<div class="microdata">\r\n     <div itemscope="" itemtype="http://schema.org/LocalBusiness">\r\n           <span itemprop="name">Southland Organics</span>\r\n     <div itemprop="address" itemscope="" itemtype="http://schema.org/PostalAddress">\r\n          <span itemprop="streetAddress">335 Barnett Shoals Road</span>\r\n          <span itemprop="addressLocality">Athens</span>, <span itemprop="addressRegion">GA</span> <span itemprop="postalCode">30605</span>\r\n     </div>\r\n     <span itemprop="telephone">(800) 608-3755</span>\r\n     <a itemprop="url" href="http://www.SouthlandOrganics.com">www.SouthlandOrganics.com</a>\r\n</div>\r\n</div>','Schema.org','full_html'),('5','<a href="#top"><div class="top-button">Back to Top</div></a><img src="/soil_conditioner/mantis.png" width="67" height="48" alt="Southland Organics Mantis" />','Back to Top Mantis','full_html');
